{
  "manifest_version": 3,
  "name": "InFox extension",
  "version": "1.0",
  "icons":{
    "16": "icons/icons8-fox-16.png",
    "32": "icons/icons8-fox-32.png",
    "128": "icons/icons8-fox-96.png"
  },
  "description": "A browser extension to highlight text and process it.",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "alarms",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
