
  {
    "manifest_version": 3,
    "name": "Text Highlighter",
    "version": "1.0",
    "description": "A browser extension to highlight text and process it.",
    "permissions": [
      "activeTab",
      "storage",
      "http://localhost:5000/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "login.html"
    }
  }
  